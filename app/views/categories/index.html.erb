<!DOCTYPE html>
<html>
	<head>
	<title>Category Types</title>
	</head>
	<body>
		<table>
			<thead>
				<tr>
					<th>Name</th>
					<th>Created</th>
					<th>Status</th>
				</tr>
			</thead>
			<tbody>
				<%@categories.each |c| %>
				<tr>
					<td><%=c.Name%></td>
					<td><%=c.Created_at%></td>
					<% if c.status == "" %>
					<td>Active</td>
					<% else %>
					<td>Inactive</td>
					<% end %>
				</tr>
			</tbody>
		</table>
	</body>
</html>